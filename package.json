    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.7.1",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.7.0",
    "koa-bearer-token": "^2.0.2",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-log4": "^2.3.2",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.1.0",
