{
  "name": "bit_palace_server",
  "version": "0.1.0",
  "author": "Baja",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./src/index.js",
    "prod": "cross-env NODE_ENV=production  nodemon ./src/index.js",
    "deploy:dev": "pm2 start ecosystem.config.js --env development",
    "deploy:prod": "pm2 start ecosystem.config.js --env production",
    "reload:prod": "pm2 reload ecosystem.config.js --env production",
    "docker:build": "docker  build -t $npm_package_name:$npm_package_version .",
    "docker:tag": "docker tag $npm_package_name:$npm_package_version $npm_package_config_dockerRepo/$npm_package_name:$npm_package_version",
    "docker:push": "docker push $npm_package_config_dockerRepo/$npm_package_name:$npm_package_version"
  },
  "dependencies": {
